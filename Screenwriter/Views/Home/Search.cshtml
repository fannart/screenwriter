@model Screenwriter.ViewModels.SearchViewModel

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th></th>
    </tr>

    @foreach (var item in Model.MostDownloaded)
    {
        <tr>
            <td>
                @item.Media.Title
            </td>
            <td>
                @item.Language.Name
            </td>
        </tr>
    }

</table>
